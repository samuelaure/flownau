{
  "name": "flownau",
  "version": "1.3.20",
  "private": true,
  "author": "Samuel Aure",
  "description": "Unified platform merging Astromatic and R2 Media Butler",
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && npm run build:remotion && next build",
    "build:remotion": "remotion bundle src/remotion/index.ts --out-dir=out --public-dir=public",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest",
    "butler": "tsx src/services/r2/butler.ts",
    "worker": "tsx src/worker.ts",
    "worker:prod": "node dist/worker.js",
    "build:worker": "tsc -p tsconfig.worker.json && tsc-alias -p tsconfig.worker.json",
    "db:migrate": "prisma migrate deploy",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "remotion:render": "remotion render src/remotion/index.ts Main out/video.mp4",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "build:seed": "esbuild scripts/seed.ts --bundle --platform=node --outfile=dist/seed.js --external:@prisma/client"
  },
  "prisma": {
    "seed": "tsx scripts/seed.ts"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.11.1",
    "@aws-sdk/client-s3": "^3.670.0",
    "@aws-sdk/lib-storage": "^3.670.0",
    "@prisma/client": "^5.21.0",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@remotion/bundler": "4.0.407",
    "@remotion/cli": "4.0.407",
    "@remotion/google-fonts": "4.0.407",
    "@remotion/player": "4.0.407",
    "@remotion/renderer": "4.0.407",
    "@remotion/zod-types": "4.0.407",
    "axios": "^1.7.7",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.67.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "fluent-ffmpeg": "^2.1.3",
    "inquirer": "^9.2.19",
    "ioredis": "^5.9.2",
    "lowdb": "^7.0.1",
    "lucide-react": "^0.453.0",
    "nanoid": "^5.0.7",
    "next": "14.2.15",
    "next-auth": "^5.0.0-beta.25",
    "next-themes": "^0.4.6",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "promise-ftp": "^1.3.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remotion": "4.0.407",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.4.0",
    "prisma": "^5.21.0",
    "zod": "3.22.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/bcryptjs": "^2.4.6",
    "@types/fluent-ffmpeg": "^2.1.26",
    "@types/inquirer": "^9.0.7",
    "@types/ioredis": "^4.28.10",
    "@types/node": "^20.16.11",
    "@types/pg": "^8.16.0",
    "@types/promise-ftp": "^1.3.9",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.23",
    "esbuild": "^0.27.2",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.15",
    "husky": "^8.0.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.3"
  }
}
